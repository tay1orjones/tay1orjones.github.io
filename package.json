{
  "name": "tay1orjones.github.io",
  "version": "2.0.0",
  "description": "chronicling my tex-mex intake on the world wide web",
  "engines": {
    "node": "4.4.5",
    "npm": "2.15.5"
  },
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/harp server ./_site & ./node_modules/.bin/browser-sync start --proxy 'localhost:9000' --files './_site/*'",
    "test": "./node_modules/.bin/sass-lint './_site/**/*.scss' --verbose --no-exit",
    "minify": "./node_modules/.bin/cleancss -o ./.dist/main-min.css ./.dist/main.css",
    "compile": "./node_modules/.bin/harp compile ./_site ./.dist && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tay1orjones/tay1orjones.github.io.git"
  },
  "author": "taylor jones",
  "license": "WTFPL",
  "bugs": {
    "url": "https://media.giphy.com/media/3o6ozoD1ByqYv7ARIk/giphy.gif"
  },
  "homepage": "https://github.com/tay1orjones/tay1orjones.github.io#readme",
  "devDependencies": {
    "browser-sync": "2.13.0",
    "clean-css": "3.4.18",
    "harp": "0.20.3",
    "sass-lint": "1.7.0"
  }
}
